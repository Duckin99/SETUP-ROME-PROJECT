{
  "name": "setup-rome-project",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "protoc": "npx proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=src/proto/ ./src/proto/item.proto ./src/proto/user.proto",
    "userServer": "tsx src/users/server",
    "userClient": "tsx src/users/client",
    "itemServer": "tsx src/items/server",
    "itemClient": "tsx src/items/client",
    "db.create": "docker create --name rome-project-db --env-file .env -p 5432:5432 bitnami/postgresql",
    "db.start": "docker start -i rome-project-db",
    "generate": "npx prisma generate",
    "db.push": "npx prisma db push",
    "db.seed": "npx tsx ./prisma/seed.ts",
    "db.studio": "npx prisma studio",
    "db.migrate": "npx prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.13.1",
    "@grpc/proto-loader": "^0.7.13",
    "google-protobuf": "^3.21.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}